{
  "name": "apple-code-assistant",
  "version": "1.0.0",
  "description": "A TypeScript CLI tool for code generation using Apple's Foundation Models",
  "main": "dist/index.js",
  "bin": {
    "apple-code": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "typescript",
    "apple",
    "foundation-models",
    "code-generation",
    "ai"
  ],
  "author": "Apple Code Assistant",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "highlight.js": "^11.9.0",
    "clipboardy": "^4.0.0",
    "fs-extra": "^11.2.0",
    "dotenv": "^16.3.1",
    "ora": "^7.0.1",
    "boxen": "^7.1.1",
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "terminal-kit": "^3.0.0",
    "cli-progress": "^3.12.0",
    "figlet": "^1.7.0",
    "gradient-string": "^2.0.2",
    "conf": "^11.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/blessed": "^0.1.25",
    "@types/figlet": "^1.5.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
